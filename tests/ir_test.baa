ثابت صحيح حد = 5.

صحيح جمع_اثنين(صحيح أ, صحيح ب) {
    إرجع أ + ب.
}

صحيح الرئيسية() {
    صحيح س = ١٠.
    صحيح ص = ٢٠.
    صحيح ن = ٠.

    // if / else
    إذا (س < ص) {
        س = س + ١.
    } وإلا {
        س = س - ١.
    }

    // while
    طالما (ن < حد) {
        ن = ن + ١.
    }

    // for
    لكل (صحيح ع = ٠؛ ع < ٣؛ ع = ع + ١) {
        س = س + ع.
    }

    // switch
    اختر (ن) {
        حالة ٠:
            س = س + ١.
            توقف.
        حالة ٣:
            س = س + ٣.
            توقف.
        افتراضي:
            س = س + ٩.
            توقف.
    }

    // call
    ص = جمع_اثنين(س, ص).

    اطبع ص.
    إرجع ص.
}