;; نواة باء - tests\ir_test.baa

;; المتغيرات العامة
ثابت عام @حد = ص٦٤ ٣

دالة @جمع_اثنين(ص٦٤ %معامل٠، ص٦٤ %معامل١) -> ص٦٤ {
بداية:
    %م١ = حجز ص٦٤
    خزن ص٦٤ %معامل٠، %م١
    %م٣ = حجز ص٦٤
    خزن ص٦٤ %معامل١، %م٣
    %م٤ = حمل ص٦٤، %م١
    %م٥ = حمل ص٦٤، %م٣
    %م٦ = جمع ص٦٤ %م٤، %م٥
    رجوع ص٦٤ %م٦
}

دالة @الرئيسية() -> ص٦٤ {
بداية:
    %م٠ = حجز ص٦٤
    خزن ص٦٤ ١٠، %م٠
    %م١ = حجز ص٦٤
    خزن ص٦٤ ٢٠، %م١
    %م٢ = حجز ص٦٤
    خزن ص٦٤ ٠، %م٢
    %م٣ = حمل ص٦٤، %م٠
    %م٤ = حمل ص٦٤، %م١
    %م٥ = قارن أصغر ص٦٤ %م٣، %م٤
    قفز_شرط %م٥، %فرع_صواب_٠، %فرع_خطأ_٢
فرع_صواب_٠:
    %م٦ = حمل ص٦٤، %م٠
    %م٧ = جمع ص٦٤ %م٦، ١
    خزن ص٦٤ %م٧، %م٠
    قفز %دمج_١
دمج_١:
    قفز %حلقة_تحقق_٣
فرع_خطأ_٢:
    %م٨ = حمل ص٦٤، %م٠
    %م٩ = طرح ص٦٤ %م٨، ١
    خزن ص٦٤ %م٩، %م٠
    قفز %دمج_١
حلقة_تحقق_٣:
    %م١٠ = حمل ص٦٤، %م٢
    %م١١ = حمل ص٦٤، @حد
    %م١٢ = قارن أصغر ص٦٤ %م١٠، %م١١
    قفز_شرط %م١٢، %حلقة_جسم_٤، %حلقة_نهاية_٥
حلقة_جسم_٤:
    %م١٣ = حمل ص٦٤، %م٢
    %م١٤ = جمع ص٦٤ %م١٣، ١
    خزن ص٦٤ %م١٤، %م٢
    قفز %حلقة_تحقق_٣
حلقة_نهاية_٥:
    %م١٥ = حجز ص٦٤
    خزن ص٦٤ ٠، %م١٥
    قفز %لكل_تحقق_٦
لكل_تحقق_٦:
    %م١٦ = حمل ص٦٤، %م١٥
    %م١٧ = قارن أصغر ص٦٤ %م١٦، ٣
    قفز_شرط %م١٧، %لكل_جسم_٧، %لكل_نهاية_٩
لكل_جسم_٧:
    %م١٨ = حمل ص٦٤، %م٠
    %م١٩ = حمل ص٦٤، %م١٥
    %م٢٠ = جمع ص٦٤ %م١٨، %م١٩
    خزن ص٦٤ %م٢٠، %م٠
    قفز %لكل_زيادة_٨
لكل_زيادة_٨:
    %م٢١ = حمل ص٦٤، %م١٥
    %م٢٢ = جمع ص٦٤ %م٢١، ١
    خزن ص٦٤ %م٢٢، %م١٥
    قفز %لكل_تحقق_٦
لكل_نهاية_٩:
    %م٢٣ = حمل ص٦٤، %م٢
    %م٢٤ = قارن يساوي ص٦٤ %م٢٣، ٠
    قفز_شرط %م٢٤، %حالة_١١، %فحص_١٤
نهاية_اختر_١٠:
    %م٣٢ = حمل ص٦٤، %م٠
    %م٣٣ = حمل ص٦٤، %م١
    %م٣٤ = نداء @جمع_اثنين(%م٣٢، %م٣٣)
    خزن ص٦٤ %م٣٤، %م١
    %م٣٥ = حمل ص٦٤، %م١
    نداء @اطبع(%م٣٥)
    %م٣٦ = حمل ص٦٤، %م١
    رجوع ص٦٤ %م٣٦
حالة_١١:
    %م٢٦ = حمل ص٦٤، %م٠
    %م٢٧ = جمع ص٦٤ %م٢٦، ١
    خزن ص٦٤ %م٢٧، %م٠
    قفز %نهاية_اختر_١٠
حالة_١٢:
    %م٢٨ = حمل ص٦٤، %م٠
    %م٢٩ = جمع ص٦٤ %م٢٨، ٣
    خزن ص٦٤ %م٢٩، %م٠
    قفز %نهاية_اختر_١٠
افتراضي_١٣:
    %م٣٠ = حمل ص٦٤، %م٠
    %م٣١ = جمع ص٦٤ %م٣٠، ٩
    خزن ص٦٤ %م٣١، %م٠
    قفز %نهاية_اختر_١٠
فحص_١٤:
    %م٢٥ = قارن يساوي ص٦٤ %م٢٣، ٣
    قفز_شرط %م٢٥، %حالة_١٢، %افتراضي_١٣
}

